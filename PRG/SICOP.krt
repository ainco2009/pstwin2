  IF PARAMETERS() < 1
    RETURN("999999999")
  ENDIF
        
  lcID = ""
  lcOldReprocess = SET('REPROCESS')
  
  *-- Lock until user presses Esc
  SET REPROCESS TO AUTOMATIC

  IF !USED("Parametro")
