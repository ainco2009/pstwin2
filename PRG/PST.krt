    pnChildRec=recno()
    pcChildID=COD_CASA
    pcChildExpr="COD_CASA"
    IF NOT llRetVal
      EXIT
    ENDIF && not llretval
    llRetVal=riupdate("COD_CASA",lcParentID,"CASACOME")
  ENDSCAN get all of the com_prom records
